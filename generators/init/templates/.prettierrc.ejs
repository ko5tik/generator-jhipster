# Prettier configuration

printWidth: 140
singleQuote: true
tabWidth: <%= prettierDefaultIndent %>
useTabs: false

# js and ts rules:
arrowParens: avoid

# jsx and tsx rules:
jsxBracketSameLine: false

# java rules:
overrides:
  - files: "*.java"
    options:
      tabWidth: <%= prettierJavaIndent %>
